    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
            font-family: 'Georgia', 'Times New Roman', serif;
        }
        
        body {
            background: linear-gradient(135deg, #0a0a0a, #1a1a2e);
            color: #e0e0e0;
            min-height: 100vh;
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            padding: 20px;
            line-height: 1.7;
        }
        
        .container {
            max-width: 1000px;
            width: 100%;
            background: rgba(15, 15, 25, 0.95);
            border-radius: 8px;
            padding: 50px;
            box-shadow: 0 15px 35px rgba(0, 0, 0, 0.6);
            border: 1px solid #2a2a3a;
            position: relative;
            overflow: hidden;
        }
        
        .container::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 4px;
            background: linear-gradient(90deg, #00ff00, #ff0000);
        }
        
        .header {
            text-align: center;
            margin-bottom: 40px;
            padding-bottom: 25px;
            border-bottom: 1px solid #333344;
        }
        
        .main-title {
            font-size: 3.2rem;
            color: #00ff00;
            margin-bottom: 15px;
            letter-spacing: 1.5px;
            font-weight: 700;
        }
        
        .message-block {
            font-size: 1.25rem;
            text-align: justify;
        }
        
        .message-block p {
            margin-bottom: 25px;
            text-indent: 1.5em;
        }
        
        .highlight {
            color: #00ff00;
            font-weight: bold;
        }
        
        .warning {
            color: #ff6666;
            font-weight: bold;
        }
        
        .emphasis {
            font-style: italic;
            color: #cccccc;
            border-left: 3px solid #00cc00;
            padding-left: 15px;
            margin: 25px 0;
        }
        
        .signature {
            text-align: right;
            margin-top: 40px;
            font-style: italic;
            color: #ff3333;
            font-size: 1.3rem;
            border-top: 1px solid #333344;
            padding-top: 20px;
        }
        
        .footer {
            text-align: center;
            margin-top: 30px;
            color: #8888aa;
            font-size: 0.9rem;
        }
        
        .seal {
            display: inline-block;
            margin: 0 10px;
            color: #00cc00;
        }
        
        .banner-image {
            width: 100%;
            max-width: 800px;
            height: 200px;
            margin: 0 auto 30px;
            border-radius: 5px;
            overflow: hidden;
            background: linear-gradient(45deg, #0a2a0a, #002200);
            display: flex;
            align-items: center;
            justify-content: center;
            border: 1px solid #00aa00;
            box-shadow: 0 0 20px rgba(0, 255, 0, 0.3);
        }
        
        .banner-image img {
            max-width: 100%;
            max-height: 100%;
            object-fit: contain;
        }
        
        @media (max-width: 768px) {
            .main-title {
                font-size: 2.3rem;
            }
            
            .message-block {
                font-size: 1.1rem;
            }
            
            .container {
                padding: 30px 20px;
            }
            
            .banner-image {
                height: 150px;
            }
        }
    </style>
</head>
