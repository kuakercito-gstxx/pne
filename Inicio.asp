            <p>#UnHackeoUnMensaje</p>
            <p class="signature">gstxx | DEFACE PERÚ</p>
        </div>
        
        <div class="footer">
            <span class="seal">■</span> Mensaje para Perú y Arequipa <span class="seal">■</span>
        </div>
    </div>

    <script>
        document.addEventListener('DOMContentLoaded', function() {
            const paragraphs = document.querySelectorAll('.message-block p, .emphasis');
            
            paragraphs.forEach(p => {
                p.style.opacity = '0';
                p.style.transform = 'translateY(20px)';
            });
            
            paragraphs.forEach((p, index) => {
                setTimeout(() => {
                    p.style.transition = 'opacity 1s ease, transform 1s ease';
                    p.style.opacity = '1';
                    p.style.transform = 'translateY(0)';
                }, 300 + (index * 400));
            });
            
            const banner = document.querySelector('.banner-image');
            setInterval(() => {
                banner.style.boxShadow = '0 0 25px rgba(0, 255, 0, 0.7)';
                setTimeout(() => {
                    banner.style.boxShadow = '0 0 20px rgba(0, 255, 0, 0.3)';
                }, 600);
            }, 3000);
        });
    </script>
</body>
</html>
